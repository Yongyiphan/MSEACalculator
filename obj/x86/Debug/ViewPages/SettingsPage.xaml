<Page
    x:Class="MSEACalculator.ViewPages.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSEACalculator.ViewPages"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:ConnectionId='2' x:Name="timelapsed" Width="200" Height="200" VerticalAlignment="Top" HorizontalAlignment="Left"></TextBlock>
        <Button x:ConnectionId='3' x:Name="uploadDb" Content="Upload" VerticalAlignment="Center" HorizontalAlignment="Center"                       ></Button>
        <Button x:ConnectionId='4' x:Name="ResetBtn" Content="Reset" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="40"                        />
    </Grid>
</Page>

